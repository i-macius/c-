Random r = new Random();
int[] T = new int[20];
for (int i = 0; i < T.Length; i++)
{
    T[i] = r.Next(1,20);
}
for (int i = 0; i < T.Length; i++)
{
    Console.Write(T[i] + " ");
}
int maks = T[0];
int ilosc = 1;
for (int i = 0; i < T.Length; i++)
{
    if (T[i] > maks)
    {
        maks = T[i];
        ilosc = 1;
    }
    else if (T[i] == maks)
    {
        ilosc += 1;
    }    
}
Console.WriteLine();
Console.WriteLine(maks + " - " + ilosc);


