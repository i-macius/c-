//Z.8
Random r = new Random();
int[] T = new int[20];
for (int i = 0; i < T.Length; i++)
{
    T[i] = r.Next(1,20);
}
for (int i = 0; i < T.Length; i++)
{
    Console.Write(T[i] + " ");
}
int parzyste = 0;
for (int i = 0; i < T.Length; i++)
{
    if (T[i] % 2 == 0)
    {
        parzyste += 1;
    }
}
int nieparzyste = 20 - parzyste;
Console.WriteLine("\n\nParzystych: " + parzyste + " Nieparzyste: " + nieparzyste);
if (nieparzyste > parzyste) Console.WriteLine("wiecej jest nie parzystych");
if (nieparzyste == parzyste) Console.WriteLine("jest tyle samo");
if (nieparzyste < parzyste) Console.WriteLine("wiecej jest parzystych");


